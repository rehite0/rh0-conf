snippet #<	"include header"
#include <${1:stdio}.h>
endsnippet
snippet #d "define macro"
#define $1 $2
endsnippet
snippet p "printf"
printf("$1");
endsnippet
snippet { "start block"
{
	$1
}
endsnippet
snippet /* "multiline comment"
/*********************************************************\
$1
\*********************************************************/
endsnippet
